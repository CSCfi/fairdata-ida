			<unsigned>true</unsigned>
			<notnull>true</notnull>
		</field>
		<field>
			<name>lastmodified</name>
			<type>integer</type>
			<unsigned>true</unsigned>
		</field>
		<field>
			<name>etag</name>
			<type>text</type>
			<length>32</length>
		</field>
		<field>
			<name>size</name>
			<type>integer</type>
			<notnull>true</notnull>
